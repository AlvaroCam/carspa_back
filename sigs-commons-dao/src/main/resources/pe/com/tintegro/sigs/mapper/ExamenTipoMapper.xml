<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pe.com.tintegro.sigs.mapper.ExamenTipoMapper" >
	<resultMap type="pe.com.tintegro.sigs.entidad.general.Item" id="examenTipoResultMap">
		<result property="ID" column="id" />
		<result property="valor" column="descripcion" />
	</resultMap>
	
	<select id="obtenerExamenTipos" resultMap="examenTipoResultMap">
		SELECT *  FROM "Admision".fn_ad_select_examene_tipo(#{ idExamenTipo })
	</select>
</mapper>