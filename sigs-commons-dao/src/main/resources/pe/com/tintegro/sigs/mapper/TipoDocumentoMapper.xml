<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
		
<mapper namespace="pe.com.tintegro.sigs.mapper.TipoDocumentoMapper">
	<resultMap id="tipoDocumentoResultMap" type = "pe.com.tintegro.sigs.entidad.general.Item">
		<result property = "ID" column = "id"/>
		<result property = "valor" column = "descripcion"/>
	</resultMap>
	
	<!-- MAPEADO EXCEL-->
	<select id = "obtenerTipoDocumento" resultMap = "tipoDocumentoResultMap">
		SELECT * FROM "Admision".fn_ad_select_tb_tipo_documento_identidad()
	</select>
</mapper>	
	